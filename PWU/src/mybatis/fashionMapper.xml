<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="fashion">
	<insert id="insert" parameterType="vo.fashionBoardVo">
		insert into f_dramaboard(num, title, p_name, p_price, content, path,reg_date )
		values(f_dramaboard_num.NEXTVAL,#{title},#{p_name},#{p_price},#{content},#{path}, sysdate)
	</insert>
	
	<select id="selectList" resultType="vo.fashionBoardVo">
		select * from f_dramaboard 
	</select>
	
	<delete id="delete" parameterType="String">
		delete from f_dramaboard where num = #{num}
	</delete>
	
	<select id="select" parameterType="String" resultType="vo.fashionBoardVo">
		select * from f_dramaboard where num=#{num}
	</select>


</mapper>

